    <string name="first_launch_permissions_title">Soundscapen avulla suunnistaminen</string>
    <string name="first_launch_permissions_message">Kun suunnistat Soundscapen avulla, sinua opastaa äänimajakka. Sinun on annettava seuraavat luvat Soundscapelle voidaksesi tätä varten:</string>
    <string name="first_launch_permissions_location">Sijaintipalvelut tarkalla sijainnilla</string>
